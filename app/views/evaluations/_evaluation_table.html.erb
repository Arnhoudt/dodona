<table class="table table-resource">
  <thead>
    <tr>
      <th><%= t('.user') %></th>
      <% feedbacks[:exercises].each do |exercise| %>
        <th class="feedback-table-center"><%= exercise.name %></th>
      <% end %>
    </tr>
  </thead>
  <tbody>
    <% evaluation.users.order(last_name: :asc, first_name: :asc).each do |user| %>
      <% feedback_l = feedbacks[:feedbacks][user.id] %>
      <tr>
        <td><%= link_to user.full_name, course_member_path(@evaluation.series.course, user), title: user.full_name, class: "ellipsis-overflow", target: "_blank" %></td>
        <% feedback_l.each do |feedback| %>
          <td class="feedback-table-center">
            <%= render partial: 'feedback_status', locals: { evaluation: feedback.evaluation, feedback: feedback } %>
          </td>
        <% end %>
      </tr>
    <% end %>
  </tbody>
</table>
