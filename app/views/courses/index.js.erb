<% if @repository %>
document.getElementById("courses-table-wrapper").innerHTML= "<%= escape_javascript(render partial: 'courses/repo_courses_table', locals: {courses: @courses}) %>"
if (dodona.repositoryCoursesLoaded) {
    dodona.repositoryCoursesLoaded();
}
<% elsif @copy_courses %>
document.getElementById("courses-table-wrapper").innerHTML= "<%= escape_javascript(render partial: 'courses/copy_courses_table', locals: {courses: @courses}) %>"
if (dodona.copyCoursesLoaded) {
    dodona.copyCoursesLoaded();
}
<% else %>
document.getElementById("courses-table-wrapper").innerHTML= "<%= escape_javascript(render partial: 'courses/courses_table', locals: {courses: @courses}) %>"
<% end %>
